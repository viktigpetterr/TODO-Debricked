        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "cross-env": "^7.0",
        "laravel-mix": "3.0.0",
        "lodash": "^4.17.19",
        "resolve-url-loader": "2.3.2",
        "sass": "^1.27.0",
        "sass-loader": "^10.0.3",
        "vue-template-compiler": "^2.6.12"
    }
